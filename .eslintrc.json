{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error"
        ],
        "no-console": "off",
        "no-restricted-syntax": "off",
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true, "packageDir": "./"}]
    },
    "ignorePatterns": [
        "frontend/**",
        "lib/**",
        "node_modules/**",
        "dist/**"
    ]
}
